[user]
	name = Julien Ponge
	email = jponge@redhat.com
	signingkey = 99447033FD4E69B4

[color]
	ui = true

[push]
	default = matching

[rerere]
	enabled = true

[credential]
	helper = osxkeychain
[core]
	excludesfile = /Users/jponge/.gitignore_global
	autocrlf = input
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/jponge/.stCommitMsg
	gpgsign = true
[pull]
	rebase = false
[init]
	defaultBranch = main
